{
    train_subset: train
    valid_subset: dev
    major_metric: top5_acc
    ascending_metric: true
    log_interval: 40
    eval_interval: 4000
    eval_interval_warmup: 0

    batch_size: 128

    lr_scheduler: one_cycle
        warmup_steps: 0.075
    optimizer: adam
        learning_rate: 1e-5
        adam_betas: [0.9, 0.999]
        adam_eps: 1e-6
        clip_norm: 1.0
        weight_decay: 0.01
}