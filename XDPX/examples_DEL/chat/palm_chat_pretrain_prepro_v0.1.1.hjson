{
    __pytorch__: 180
    __v100__: n
    __worker__: 1
    __gpu__: 1
    __cpu__: 40
    __memory__: 40
    __parent__: [
        [
            {__def__: {task_name: 'v0.1'} } #
        ]
    ]
    __def__: {
        source_root: ../datahub/data
        data_root: ../datahub/process_data
    }

    data_files: ['*.jsonl']
    data_source: ${source_root}/${task_name}
    data_dir:  ${data_root}/${task_name}
