    save_format: torch

    parser: jsonl
    fields: ['session','source']
    loader: session_chat
    processor: session_chat
    tokenizer: bert
    vocab_file: tests/sample_data/palm_vocab.txt
    cls_index: 101
    sep_index: 102

    target_map_file: tests/sample_data/noop_target_map.txt

    skip_bad_lines:true

    max_len: 300
}
