{
    __pytorch__: 180
    __v100__: n
    __worker__: 1
    __gpu__: 1
    __cpu__: 40
    __memory__: 40
    __parent__: [
        [
            {__def__: {task_name: 'v1.1.3'} } #
        ]
    ]
    __def__: {
        source_root: oss://xdp-expriment/jiayi.qm/0_digital_human/data
        data_root: oss://xdp-expriment/zhimiao.chh/data/dialogue/finetune
    }

    data_files: ['*.json']
    data_source: ${source_root}/${task_name}
    data_dir:  ${data_root}/${task_name}-typeid
    workers: 64
    save_format: torch

    parser: jsonl
    fields: ['context','response','passages']
    loader: plugv2_fidchat
    processor: plugv2_fidchat
    tokenizer: palm_tokenizer
    vocab_file: tests/sample_data/palm_vocab.txt
    pad_word: '[PAD]'
    unk_word: '[UNK]'

    target_map_file: tests/sample_data/noop_target_map.txt

    skip_bad_lines:true

    max_len: 300
    max_encoder_length: 300
    max_decoder_length: 80
}
