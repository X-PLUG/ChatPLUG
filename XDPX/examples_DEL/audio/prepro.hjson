{
    __pytorch__: 180
    __v100__: n
    __worker__: 1
    __gpu__: 1
    __cpu__: 40
    __memory__: 80


    __def__: {
        source_root: oss://xdp-expriment/gaoxing.gx/dialoglue/Datasets/BANKING77_yue/
        // data_root: oss://use/your/own/directory
        task_type:'yue-asr-audio'
    }
    max_len: 100
    data_files: *.json

    data_source: ${source_root}/${task_type}
    data_dir:  ${data_root}/${task_type}-noises
    audio_dir: ${source_root}/noise-audios/

    parser: json
    fields: ['text','audio','label']
    loader: single_audio_text
    processor: bert_single_wav2vec
    wav2vec_processor: facebook/wav2vec2-large-xlsr-53

    target_type: text
    max_audio_length: 100000

    workers: 4
    special_targets:[]

    vocab_file: xlm-roberta-base
    tokenizer: auto
    pad_word: <pad>
    unk_word: <unk>
    lower: false

}
